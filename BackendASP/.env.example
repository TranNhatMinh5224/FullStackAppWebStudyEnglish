# Frontend Configuration
Frontend__BaseUrl=http://localhost:3000

# Database Configuration (PostgreSQL)
POSTGRES_PASSWORD=YOUR_POSTGRES_PASSWORD
ConnectionStrings__DefaultConnection=Host=localhost;Port=5432;Database=LearningEnglishDb;Username=postgres;Password=YOUR_PASSWORD

# JWT Configuration
# Key must be at least 32 characters
Jwt__Key=YOUR_SECRET_KEY_HERE_MIN_32_CHARS
Jwt__Issuer=ELearningEnglish
Jwt__Audience=ELearningEnglishAudience

# SMTP Configuration (Email)
Smtp__Host=smtp.gmail.com
Smtp__Port=587
Smtp__User=YOUR_EMAIL@gmail.com
Smtp__Password=YOUR_APP_PASSWORD
Smtp__EnableSsl=true
Smtp__FromName=E-Learning English Platform

# Azure Speech Service (Pronunciation Assessment)
AzureSpeech__SubscriptionKey=YOUR_AZURE_SPEECH_KEY
AzureSpeech__Region=YOUR_AZURE_REGION

# MinIO Configuration (Object Storage)
MinIO__Endpoint=localhost:9000
MinIO__AccessKey=YOUR_MINIO_ACCESS_KEY
MinIO__SecretKey=YOUR_MINIO_SECRET_KEY
MinIO__UseSSL=false
Minio__BaseUrl=http://localhost:9000

# Oxford Dictionary API
OxfordDictionary__BaseUrl=https://od-api-sandbox.oxforddictionaries.com/api/v2
OxfordDictionary__AppId=YOUR_OXFORD_APP_ID
OxfordDictionary__AppKey=YOUR_OXFORD_APP_KEY

# Unsplash API (Images)
Unsplash__ApplicationId=YOUR_UNSPLASH_APP_ID
Unsplash__AccessKey=YOUR_UNSPLASH_ACCESS_KEY
Unsplash__SecretKey=YOUR_UNSPLASH_SECRET_KEY
Unsplash__BaseUrl=https://api.unsplash.com

# Google OAuth2
GoogleAuth__ClientId=YOUR_GOOGLE_CLIENT_ID
GoogleAuth__ClientSecret=YOUR_GOOGLE_CLIENT_SECRET
GoogleAuth__RedirectUri=http://localhost:3000/auth/google/callback

# Facebook OAuth2
FacebookAuth__AppId=YOUR_FACEBOOK_APP_ID
FacebookAuth__AppSecret=YOUR_FACEBOOK_APP_SECRET
FacebookAuth__RedirectUri=http://localhost:3000/auth/facebook/callback

# PayOS Configuration (Payment Gateway)
PayOS__ClientId=YOUR_PAYOS_CLIENT_ID
PayOS__ApiKey=YOUR_PAYOS_API_KEY
PayOS__ChecksumKey=YOUR_PAYOS_CHECKSUM_KEY
PayOS__ApiUrl=https://api-merchant.payos.vn
PayOS__ReturnUrl=http://localhost:5030/api/user/payments/payos/return
PayOS__CancelUrl=http://localhost:5030/api/user/payments/payos/return
PayOS__WebhookUrl=YOUR_WEBHOOK_URL

# Gemini AI Configuration (AI Essay Grading)
Gemini__ApiKey=YOUR_GEMINI_API_KEY
Gemini__Model=gemini-2.0-flash-exp
Gemini__BaseUrl=https://generativelanguage.googleapis.com/v1beta
Gemini__MaxTokens=2048
Gemini__Temperature=0.3
